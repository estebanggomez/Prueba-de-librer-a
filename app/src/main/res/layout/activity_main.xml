<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/TvLastTx"
        android:layout_width="361dp"
        android:layout_height="543dp"
        android:text="TextView"
        app:layout_constraintTop_toBottomOf="@+id/transactions_text"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="21dp" />

    <TextView
        android:id="@+id/TvTipo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="76dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/tipo_text"
        app:layout_constraintTop_toBottomOf="@+id/TvId"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/TvSaldo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="64dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/saldo_text"
        app:layout_constraintTop_toBottomOf="@+id/TvTipo"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/title_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/id_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ID:"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="7dp"
        tools:layout_editor_absoluteY="24dp" />

    <TextView
        android:id="@+id/tipo_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tipo:"
        app:layout_constraintTop_toBottomOf="@+id/id_text"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="7dp" />

    <TextView
        android:id="@+id/saldo_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="saldo: "
        app:layout_constraintTop_toBottomOf="@+id/tipo_text"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="7dp" />

    <TextView
        android:id="@+id/transactions_text"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:maxLines="10"
        android:text="LastTransactions: "
        app:layout_constraintTop_toBottomOf="@+id/saldo_text"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/TvId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="92dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/id_text"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="24dp" />

</androidx.constraintlayout.widget.ConstraintLayout>